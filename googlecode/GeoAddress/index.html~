 <script>
 var lat = 0;
 var lon = 0;
 $('#form').submit(function(e){
	e.preventDefault();
	$.ajax({

		url:"http://nominatim.openstreetmap.org/search?format=json&" + $('#form').serialize(),
		type:'GET',
		dataType: 'json',
		
		success:function(data,status){
			locationData=data;
			console.log(data);
			lat = data[0].lat;
			lon = data[0].lon;
			console.log(lat);
			console.log("http://staticmap.openstreetmap.de/staticmap.php?center="+lat+"&"+lon+"&zoom=6&size=300x300&maptype=mapnik");
			$("#inf span").text(data[0].display_name + " is a "
			+ data[0].type + " located at lattitude " 
			+lat+ ", longitude "+lon + ".");
			$('#inf img').attr("src","http://staticmap.openstreetmap.de/staticmap.php?center="+lat+","+lon+"&zoom=10&size=300x300&maptype=mapnik");

			}
		});
 });
</script>


<script>
		window.onload = function(){
			var a = document.getElementById("licenselink");
			a.onclick = function() {
				
				$("#lic span").text( 	"The MIT License (MIT)"
